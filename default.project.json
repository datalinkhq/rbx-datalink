{
	"name": "DataLink-SDK",
	"tree": {
		"$className": "DataModel",

		"HttpService": {
			"$className": "HttpService",
			"$properties": { "HttpEnabled": true }
		},

		"ServerStorage": {
			"$className": "ServerStorage",
			
			"DatalinkSDK": {
				"$path": "source",
				"$ignoreUnknownInstances": true,

				"Submodules": {
					"$className": "Folder",

					"Janitor": { "$path": "submodules/Janitor" },
					"Promise": { "$path": "submodules/Promise" },
					"Sift": { "$path": "submodules/Sift" }
				}
			}
		},

		"ServerScriptService": {
			"$className": "ServerScriptService",
			
			"DatalinkSDK-Tests": { 
				"$path": "tests",
				"$ignoreUnknownInstances": true,

				"Submodules": {
					"$className": "Folder",

					"TestEZ": { "$path": "submodules/TestEZ" }
				}
			}
		}
	}
}